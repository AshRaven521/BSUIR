#pragma hdrstop

#include "Unit2.h"
void List::AddList(std::string str,int a,int b)
{
	Products *t;
	t=head;
	t->next = new node;
	t = t->next;
	t->name = str ;
	t->quantity = a;
	t->workshop_number = b;
	t->next = NULL;
}

